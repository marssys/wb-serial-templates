{
    "title": "Princ Gas Meter (RS-485/232 adapter, Modbus)",
    "device_type": "princ_gas_meter_adapter",
    "group": "g-custom",
    "device": {
        "name": "Princ Gas Meter",
        "id": "princ_gas_meter",
        "guard_interval_us": 5000,
        "channels": [
            {
                "name": "Volume I, m3",
                "id": "volume_i",
                "reg_type": "input",
                "address" : "0",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "group": "main"
            },
            {
                "name": "Volume F, mm3",
                "id": "volume_f",
                "reg_type": "input",
                "address" : "2",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "group": "main"
            },
            {
                "name": "Flow, mL per hour",
                "id": "flow",
                "reg_type": "input",
                "address" : "40",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "group": "main"
            },
            {
                "name": "Operating Volume I, m3",
                "id": "op_volume_i",
                "reg_type": "input",
                "address" : "20",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "enabled": false,
                "group": "main"
            },
            {
                "name": "Operating Volume F, mm3",
                "id": "op_volume_f",
                "reg_type": "input",
                "address" : "22",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "enabled": false,
                "group": "main"
            },
            {
                "name": "Operating Flow, mL per hour",
                "id": "op_flow",
                "reg_type": "input",
                "address" : "24",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "enabled": false,
                "group": "main"
            },
            {
                "name": "Volume Coefficient",
                "id": "vol_coefficient",
                "reg_type": "input",
                "address" : "17",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "scale": 0.001,
                "enabled": false,
                "group": "main"
            },
            {
                "name": "Gas Temperature",
                "id": "gas_temperature",
                "reg_type": "input",
                "address" : "19",
                "type": "temperature",
                "format": "s16",
                "scale": 0.1,
                "enabled": false,
                "group": "main"
            },
            {
                "name": "Operating Time, secs",
                "id": "operating_time",
                "reg_type": "input",
                "address" : "4",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "enabled": false,
                "group": "time"
            },
            {
                "name": "Fault Time, secs",
                "id": "fault_time",
                "reg_type": "input",
                "address" : "6",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "enabled": false,
                "group": "time"
            },
            {
                "name": "Time After Check, secs",
                "id": "time_after_check",
                "reg_type": "input",
                "address" : "8",
                "type": "value",
                "format": "u32",
                "word_order": "little_endian",
                "enabled": false,
                "group": "time"
            },
            {
                "name": "Battery Voltage, mV",
                "id": "battery_voltage",
                "reg_type": "input",
                "address" : "36",
                "type": "value",
                "format": "u16",
                "enabled": false,
                "group": "battery"
            },
            {
                "name": "Battery Rest, months",
                "id": "battery_rest",
                "reg_type": "input",
                "address" : "16",
                "type": "value",
                "format": "u16",
                "enabled": false,
                "group": "battery"
            }
        ],
        "groups": [
            {
                "title": "Main",
                "id": "main",
                "order": 1
            },
            {
                "title": "Time",
                "id": "time",
                "order": 2
            },
            {
                "title": "Battery",
                "id": "battery",
                "order": 3
            }
        ]
    }
}